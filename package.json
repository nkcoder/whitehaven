{
  "name": "whitehaven",
  "version": "1.0.0",
  "description": "This project includes a AWS Lambda function, which consumes messages from AWS SQS, get data from AWS DynamoDB, then notify remote systems by calling a webhook API.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "sls offline --stage local",
    "build": "tsc",
    "deploy": "sls deploy",
    "prune": "sls prune",
    "remove": "sls remove",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "check": "biome check --write",
    "test": "vitest run",
    "test_ui": "vitest --ui",
    "test_coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.879.0",
    "@aws-sdk/client-sqs": "^3.879.0",
    "@aws-sdk/lib-dynamodb": "^3.879.0",
    "@types/aws-lambda": "^8.10.152",
    "aws-lambda": "^1.0.7",
    "change-case": "^5.4.4",
    "date-fns": "^4.1.0",
    "ky": "^1.9.1",
    "purify-ts": "^2.1.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/node": "^22.16.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "serverless-offline": "^14.4.0",
    "serverless-prune-plugin": "^2.1.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
