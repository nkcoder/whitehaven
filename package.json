{
  "name": "whitehaven",
  "version": "1.0.0",
  "description": "This project includes a AWS Lambda function, which consumes messages from AWS SQS, get data from AWS DynamoDB, then notify remote systems by calling a webhook API.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "sls offline --stage local",
    "build": "tsc",
    "deploy": "sls deploy",
    "prune": "sls prune",
    "remove": "sls remove",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "vitest run",
    "test_ui": "vitest --ui",
    "test_coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/client-sqs": "^3.854.0",
    "@aws-sdk/lib-dynamodb": "^3.850.0",
    "@types/aws-lambda": "^8.10.152",
    "aws-lambda": "^1.0.7",
    "change-case": "^5.4.4",
    "date-fns": "^4.1.0",
    "ky": "^1.8.2",
    "prettier": "^3.6.2",
    "purify-ts": "^2.1.2",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^22.16.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "serverless-offline": "^14.4.0",
    "serverless-prune-plugin": "^2.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
